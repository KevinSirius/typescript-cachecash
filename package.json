{
  "name": "cachecash",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "main": "dist/index.js",
  "browser": "browser/browser.js",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Opera versions",
    "last 2 Edge versions"
  ],
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Kevin Kelley <kelleyk@kelleyk.net>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rm -rf browser dist tsout demoout",
    "compile": "tsc -p .",
    "bundle:browser": "parcel build -d browser --out-file browser.js --global cachecash src/index.ts",
    "bundle:node": "parcel build --target node src/index.ts",
    "build": "npm run compile && npm run bundle:browser && npm run bundle:node",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "start": "npm run build && parcel serve -d demoout demo/basic/index.html",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "cachecash-curl": "./node_modules/ts-node/dist/bin.js bin/cachecash-curl.ts"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/browser/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@improbable-eng/grpc-web-node-http-transport": "0.9.1",
    "@types/detect-node": "^2.0.0",
    "@types/google-protobuf": "^3.2.7",
    "@types/jest": "^23.3.2",
    "@types/node": "^11.13.4",
    "colors": "^1.3.2",
    "commitizen": "^3.0.7",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "detect-node": "^2.0.4",
    "google-protobuf": "^3.7.1",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^8.1.5",
    "lodash.camelcase": "^4.3.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.4",
    "rimraf": "^2.6.2",
    "rollup": "^1.10.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "semantic-release": "^15.9.16",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^23.10.2",
    "ts-node": "^8.0.3",
    "ts-protoc-gen": "^0.9.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.0",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.9.1",
    "@peculiar/webcrypto": "^1.0.9"
  }
}
