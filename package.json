{
  "name": "cachecash",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "main": "dist/index.js",
  "browser": "browser/browser.js",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Opera versions",
    "last 2 Edge versions"
  ],
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Kevin Kelley <kelleyk@kelleyk.net>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rm -rf browser dist tsout demoout",
    "compile": "tsc -p .",
    "bundle:browser": "parcel build -d browser --out-file browser.js --global cachecash src/index.ts",
    "bundle:node": "parcel build --target node src/index.ts",
    "build": "npm run compile && npm run bundle:browser && npm run bundle:node",
    "lint": "tslint  --project tsconfig.json -t codeFrame --exclude src/proto/cachecash_pb.d.ts 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "start:basic": "npm run build && parcel serve -d demoout demo/basic/index.html",
    "start:bigbuckbunny": "npm run build && parcel serve -d demoout demo/bigbuckbunny/index.html",
    "start:ubuntu-iso": "npm run build && parcel serve -d demoout demo/ubuntu-iso/index.html",
    "start": "npm run start:bigbuckbunny",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "integration": "jest -c jest.config.integration.js",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "cachecash-curl": "./node_modules/ts-node/dist/bin.js bin/cachecash-curl.ts"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@improbable-eng/grpc-web-node-http-transport": "^0.11.0",
    "@types/google-protobuf": "^3.7.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "colors": "^1.3.2",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.6",
    "cross-env": "^5.2.1",
    "cz-conventional-changelog": "^3.0.2",
    "google-protobuf": "^3.9.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "lint-staged": "^9.2.5",
    "lodash.camelcase": "^4.3.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "prompt": "^1.0.0",
    "randombytes": "^2.1.0",
    "replace-in-file": "^4.1.3",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.1",
    "semantic-release": "^15.13.24",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "ts-protoc-gen": "^0.11.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.11.0",
    "aes-js": "^3.1.2",
    "sha.js": "^2.4.11"
  }
}
